# Number of students
n = int(input())

student_marks = {}

# Read student data
for _ in range(n):
    data = input().split()
    name = data[0]
    marks = list(map(float, data[1:]))
    student_marks[name] = marks

# Query student name
query_name = input()

# Calculate average
marks_list = student_marks[query_name]
average = sum(marks_list) / len(marks_list)

# Print with 2 decimal places
print("{:.2f}".format(average))
