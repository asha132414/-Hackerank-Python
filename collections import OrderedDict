from collections import OrderedDict

n = int(input())
items = OrderedDict()

for _ in range(n):
    data = input().rsplit(' ', 1)   # split only last part as price
    name = data[0]
    price = int(data[1])
    
    if name in items:
        items[name] += price
    else:
        items[name] = price

for name, total in items.items():
    print(name, total)
